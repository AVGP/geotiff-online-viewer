{
  "name": "geotiff-viewer",
  "version": "1.0.0",
  "description": "Client to visualise the compressed JSON pointcloud in Three.js",
  "main": "app.js",
  "scripts": {
    "build": "browserify js/main.js | uglifyjs -mc > ../server/public/javascripts/app.js",
