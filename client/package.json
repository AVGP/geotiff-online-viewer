  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "leapjs": "0.6.4",
    "three": "0.71.0",
    "three-world": "2.1.0"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "uglify-js": "^2.4.16",
    "watchify": "^2.3.0"
  }
}
